<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>Inertia powered page</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    {{ inertiaHead(page) }}
  </head>
  <body>
    {{ inertia(page) }}
    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
  </body>
</html>